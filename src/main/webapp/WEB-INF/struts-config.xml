<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>

	<form-beans>
		<form-bean name="turnForm" type="com.philihp.weblabora.form.TurnForm" />
	</form-beans>

	<global-forwards />

	<action-mappings>
		<action path="/showBoard" name="turnForm" type="com.philihp.weblabora.action.ShowBoard">
			<forward name="default" path="/board.jsp" />
		</action>
	</action-mappings>

</struts-config>